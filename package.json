{
  "name": "huangsam.github.io",
  "description": "Holds links to my commonly used pages",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-loader": "^8.0.5",
    "core-js": "^3.0.0",
    "css-loader": "^3.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-jest": "^25.1.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^4.0.0",
    "jest": "^25.0.0",
    "prettier": "^2.0.0",
    "puppeteer": "^3.0.0"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack --progress",
    "build:dev": "webpack --progress",
    "lint": "eslint src *.js",
    "fmt": "prettier --write src *.js",
    "test": "jest",
    "test:e2e": "jest -c jest.e2e.config.js",
    "test:watch": "jest --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn test",
      "pre-push": "yarn build"
    }
  }
}
