{
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": ["eslint:recommended", "airbnb-base"],
  "rules": {
    "comma-dangle": ["error", "always-multiline"],
    "semi": 2,
    "no-restricted-syntax": 0,
    "no-alert": 0
  },
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "jest": true
  }
}
